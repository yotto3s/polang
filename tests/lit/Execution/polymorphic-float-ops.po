; RUN: %polang_repl %s | %FileCheck %s

; Test polymorphic functions with float operations

let add(x, y) = x + y
let mul(x, y) = x * y

; Call with integers first
let int_result = add(10, 20)

; Call same function with floats
let float_result = add(1.5, 2.5)

; Test polymorphic multiplication with floats
; CHECK: 7.5 : f64
mul(2.5, 3.0)
