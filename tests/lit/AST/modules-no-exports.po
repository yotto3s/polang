; RUN: %polang_compiler --dump-ast %s | %FileCheck %s

; Test module declaration AST without exports (all private)
; CHECK:      NBlock
; CHECK-NEXT: `-NModuleDeclaration 'Private'
; CHECK-NEXT:   `-NFunctionDeclaration 'helper' (x: i64) -> i64
; CHECK-NEXT:     `-NBlock
; CHECK-NEXT:       `-NExpressionStatement
; CHECK-NEXT:         `-NBinaryOperator '*'
; CHECK-NEXT:           |-NIdentifier 'x'
; CHECK-NEXT:           `-NInteger 2

module Private
  let helper(x: i64): i64 = x * 2
endmodule
