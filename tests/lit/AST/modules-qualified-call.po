; RUN: %polang_compiler --dump-ast %s | %FileCheck %s

; Test qualified function call AST
; CHECK:      NBlock
; CHECK-NEXT: |-NModuleDeclaration 'Math' (add)
; CHECK:      `-NExpressionStatement
; CHECK-NEXT:   `-NMethodCall 'Math.add'
; CHECK-NEXT:     |-NInteger 1
; CHECK-NEXT:     `-NInteger 2

module Math (add)
  let add(x: i64, y: i64): i64 = x + y
endmodule
Math.add(1, 2)
